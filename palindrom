class Solution {
    public boolean isPalindrome(int x) {
        if(x<0)
            return false;
            int y=x;
    int rev=0,r;
            while(y>0){
            r=y%10;
            rev=rev*10+r;
            y=y/10;
            }
            if(rev==x)
            return true;
            else
                return false;
        }
    }


